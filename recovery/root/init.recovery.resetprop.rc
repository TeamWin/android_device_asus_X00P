service set-old-build /sbin/set-old-build.sh
    user root
    group root
    disabled
    oneshot
    seclabel u:r:recovery:s0

service set-product-carrier /sbin/set-product-carrier.sh
    user root
    group root
    disabled
    oneshot
    seclabel u:r:recovery:s0

on boot
    start set-old-build
    start set-product-carrier
